}
.checkallcod-list .box-buy-product{
	overflow:hidden;
	border-bottom:1px solid #ddd;
	padding:0 0 7px;
	margin:0 0 7px;
}
.checkallcod-list .box-buy-product .count-buy-product{
	float:left;
	width:30px;
	font-weight:bold;
}
.checkallcod-list .box-buy-product .desc-product{
	margin-left:30px;
	float:none !important;
}
.checkallcod-list .box-buy-product .title-product-choose{
	width:auto;
	white-space:normal;
}
.checkallcod-list .grid-form{
	margin-bottom:0;
}
/*end checkallcod*/