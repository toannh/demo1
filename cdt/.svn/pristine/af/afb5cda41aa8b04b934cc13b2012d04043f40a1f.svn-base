}
.checkallcod-list .glyphicon-remove{
	cursor:pointer;
	font-size:14px;
	margin-right:4px;
}
.checkallcod-list .glyphicon-pencil{
	cursor:pointer;
	font-size:14px;
	margin-left:4px;
}
.checkallcod-list .form-horizontal .control-label{
	text-align:left;
}
.checkallcod-list .form-horizontal .form-group{
	margin-bottom:5px;
}
.checkallcod-list .total-money-bill{
